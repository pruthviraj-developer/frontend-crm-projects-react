    "prepare": "tsdx build --format esm",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router": "5.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@hs/utils",
  "author": "Rishikant Tiwari",
  "module": "dist/utils.esm.js",
  "size-limit": [
    {
      "path": "dist/utils.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^5.0.3",
    "size-limit": "^5.0.3",
    "@types/colornames": "^1.1.1",
    "husky": "6.0.0",
    "tsdx": "0.14.1",
    "tslib": "2.2.0",
    "typescript": "4.1.2"
  },
  "dependencies": {
    "@material-ui/core": "4.11.4",
    "colornames": "^1.1.1"
  }
