{
  "version": "1.0.0",
  "license": "MIT",
  "main": "dist/utils.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development tsdx watch --format esm --verbose",
