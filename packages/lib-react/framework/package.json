{
    "version": "1.0.0",
    "license": "MIT",
    "main": "dist/framework.esm.js",
    "typings": "dist/index.d.ts",
    "files": [
        "dist",
        "src"
    ],
    "engines": {
        "node": ">=12"
    },
    "scripts": {
        "start": "tsdx watch --format esm",
        "build": "tsdx build --format esm",
        "test": "tsdx test --passWithNoTests",
        "lint": "tsdx lint",
        "prepare": "tsdx build",
        "size": "size-limit",
        "analyze": "size-limit --why"
    },
    "peerDependencies": {
        "react": "17.0.2"
    },
    "husky": {
        "hooks": {
            "pre-commit": "tsdx lint"
        }
    },
    "prettier": {
        "printWidth": 80,
        "semi": true,
        "singleQuote": true,
        "trailingComma": "es5"
    },
    "name": "@hs/framework",
    "author": "Rishikant Tiwari",
    "module": "dist/framework.esm.js",
    "size-limit": [
        {
            "path": "dist/framework.esm.js",
            "limit": "10 KB"
        }
    ],
    "devDependencies": {
        "@size-limit/preset-small-lib": "^5.0.3",
        "size-limit": "^5.0.3",
        "@types/react": "17.0.6",
        "@types/react-dom": "17.0.5",
        "husky": "6.0.0",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "tsdx": "0.14.1",
        "tslib": "2.2.0",
        "typescript": "4.1.2"
    },
    "dependencies": {}
}