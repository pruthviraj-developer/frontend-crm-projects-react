{
  "version": "1.0.0",
  "license": "MIT",
  "main": "dist/framework.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "start": "tsdx watch --format esm",
    "build": "tsdx build --format esm",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@hs/framework",
  "author": "Rishikant Tiwari",
  "module": "dist/framework.esm.js",
  "size-limit": [
    {
      "path": "dist/framework.esm.js",
      "limit": "20 KB"
    }
  ],
  "peerDependencies": {
    "react": "17.0.2",
    "@hs/services": "^1.0.0",
    "ua-parser-js": "^1.0.2"
  },
  "devDependencies": {
    "@hs/services": "^1.0.0",
    "@size-limit/preset-small-lib": "^5.0.3",
    "@types/react": "17.0.19",
    "@types/ua-parser-js": "^0.7.36",
    "husky": "6.0.0",
    "react": "17.0.2",
    "size-limit": "^5.0.3",
    "tsdx": "0.14.1",
    "tslib": "2.3.1",
    "typescript": "4.3.5",
    "ua-parser-js": "^1.0.2"
  }
}