{
    "version": "0.1.0",
    "license": "MIT",
    "main": "dist/containers.esm.js",
    "typings": "dist/index.d.ts",
    "files": [
        "dist",
        "src"
    ],
    "engines": {
        "node": ">=12"
    },
    "scripts": {
        "start": "tsdx watch --format esm",
        "build": "tsdx build --format esm",
        "test": "tsdx test --passWithNoTests",
