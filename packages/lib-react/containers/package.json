    "module": "dist/containers.esm.js",
    "size-limit": [
        {
            "path": "dist/containers.esm.js",
            "limit": "10 KB"
        }
    ],
    "peerDependencies": {
        "react": "17.0.2"
    },
    "devDependencies": {
        "@babel/core": "^7.10.3",
        "@size-limit/preset-small-lib": "^5.0.3",
        "@types/react": "17.0.19",
        "babel-loader": "^8.1.0",
        "babel-plugin-emotion": "^10.0.33",
        "cross-env": "^7.0.2",
        "eslint-config-react-app": "^5.2.1",
        "husky": "6.0.0",
        "size-limit": "^5.0.3",
        "tsdx": "0.14.1",
        "tslib": "2.2.0",
        "typescript": "4.3.5"
    },
    "dependencies": {
        "@emotion/react": "11.1.5",
